<!DOCTYPE html>

<title>ProdTest index</title>

{% block body %}
    <h1>ProdTest index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for prod_test in prod_tests %}
            <tr>
                <td>{{ prod_test.id }}</td>
                <td>{{ prod_test.name }}</td>
                <td>
                    <a href="{{ path('prod_test_show', {'id': prod_test.id}) }}">show</a>
                    <a href="{{ path('prod_test_edit', {'id': prod_test.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('prod_test_new') }}">Create new</a>
{% endblock %}
